    "@vuepress/plugin-container": "2.0.0-beta.41",
    "@vuepress/shared": "2.0.0-beta.41",
    "@vuepress/utils": "2.0.0-beta.41",
    "@vueuse/core": "^8.3.1",
    "chart.js": "^3.7.1",
    "flowchart.js": "^1.17.1",
    "katex": "^0.15.3",
    "markdown-it": "^13.0.0",
    "mermaid": "^9.0.1",
    "ts-debounce": "^4.0.0",
    "reveal.js": "^4.3.1",
